import turtle
turtle.forward(200)
turtle.right(180)
turtle.forward(400)
turtle.right(180)
turtle.forward(200)
turtle.right(90)
turtle.forward(200)
turtle.right(180)
turtle.forward(400)
turtle.penup()
turtle.goto(50, 0)
turtle.pendown()
turtle.circle(50)
turtle.penup()
turtle.goto(0,215)
turtle.write("North")
turtle.goto(225, 0)
turtle.write('East')
turtle.goto(0, -215)
turtle.write('South')
turtle.goto(-225, 0)
turtle.write('West')
turtle.hideturtle()
input('press any key to exit')