import turtle 
turtle.circle(50)
turtle.penup()
turtle.goto(-60, 50)
turtle.pendown()
turtle.circle(50)
turtle.penup()
turtle.goto(60, 50)
turtle.pendown()
turtle.circle(50)
turtle.penup()
turtle.goto(120, 0)
turtle.pendown()
turtle.circle(50)
turtle.penup()
turtle.goto(180, 50)
turtle.pendown()
turtle.circle(50)
input("press any key to exit")