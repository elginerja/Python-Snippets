import turtle
turtle.left(72)
turtle.forward(230)
turtle.right(144)
turtle.forward(230)
turtle.right(108)
turtle.forward(143)
turtle.right(135)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.penup()
turtle.goto(72, 70)
turtle.hideturtle()
turtle.fillcolor('blue')
turtle.begin_fill()
turtle.goto(0, 0)
turtle.goto(143, 0)
turtle.goto(70, 71)
turtle.end_fill()
input('press any key to exit')